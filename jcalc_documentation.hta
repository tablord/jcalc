<DIV id=sect0225 class="SECTION ELEMENT" sizcache="1" sizset="83">
<H3 class="SECTIONTITLE EDITABLE" contentEditable=false>3.4.4 edit</H3>
<DIV sizcache="1" sizset="83" container="sectionContent"><PRE id=code0227 class="CODE AUTOEDIT ELEMENT EDITABLE" contentEditable=false jcObject="newTable">table("newTable")<BR>.add({"toto":127,"tutu":1452,"somme":f("toto+tutu")})<BR>.add({"toto":133,"tutu":145,"somme":f("toto+tutu")})<BR>.edit()</PRE>
