  <h1>jCalc lib</h1>
  <div id="code0102" class="CODE" onclick="jc.codeClick(this);" contentEditable>v("hauteurTotale$mm",f("hauteur$mm+5"))<p>v("hauteur$mm",3)<p>hauteurTotale$mm</div>
  <div id="out0102" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <div id="test0102" class="TEST"><VAR>hauteurTotale</VAR>= <SPAN class=VALUE>8</SPAN><SPAN class=UNIT>mm</SPAN></div>
  <div id="code0103" class="CODE" onclick="jc.codeClick(this);" contentEditable>v("hauteur$mm",3)</div>
  <div id="out0103" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <div id="code0104" class="CODE AUTOEXEC" onclick="jc.codeClick(this);" contentEditable>v("hauteur$mm")</div>
  <div id="out0104" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <div id="code0105" class="CODE" onclick="jc.codeClick(this);" contentEditable>
    table("myTable")<p>
    &nbsp;&nbsp;.add({_id:"apple" ,quantity:250,price:0.5,amount:f("quantity*price")})<p>
    &nbsp;&nbsp;.add({_id:"orange",quantity:100,price:0.7,amount:f("quantity*price")});
  </div>
  <div id="out0105" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <div id="code0106" class="CODE" onclick="jc.codeClick(this);" contentEditable>
    output<p>
    .p("first § where we show that ",hauteurTotale$mm)<p>
    .p("second § where we show myTable",myTable.span({cols:{_id:1,amount:1}}))<p>
    .ul()<p>
    &nbsp;&nbsp;.li('list 1 the full table sorted by quantity from min to max',myTable.sort({quantity:1}))<p>
    &nbsp;&nbsp;.li('list 2 a row',myTable.orange)<p>
    &nbsp;&nbsp;.li('list 3 a row as a list',myTable.orange.list())<p>
    .end()<p>
    .h('Titre 1').indent()<p>
    &nbsp;&nbsp;.h('Sous-Titre 1')<p>
    &nbsp;&nbsp;.h('Sous-Titre 2')<p>
    .indent(-1)<p>
    .h('Titre 2')
  </div>
  <div id="out0106" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <h1>JQuery</h1>
  <div id="code0201" class="CODE" onclick="jc.codeClick(this);" contentEditable>$('#code0104')[0]</div>
  <div id="out0201" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <div id="test0201" class="TEST">DOM Element<SPAN class=INSPECTHTML>&lt;DIV onclick=jc.codeClick(this); id=code0104 class="CODE AUTOEXEC" contentEditable=true&gt;v("hauteur$mm")&lt;/DIV&gt;</SPAN>
</div>
  <h1>Axe</h1>
  <div id="code0301" class="CODE" onclick="jc.codeClick(this);" contentEditable>
    axe('lift')<p>
    .accelerate({t:5,a:1})<p>
    .accelerate({t:10,a:-1})<p>
    .accelerate({t:15,a:0})<p>
    .move({t:20,a:1,d:40,v1:5})
  </div>
  <div id="out0301" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <div id="code0302" class="CODE" onclick="jc.codeClick(this);" contentEditable>
    lift<p>
    .accelerate({t:33,a:0})<p>
  </div>
  <div id="out0302" class="OUTPUT" onclick="jc.outClick(this)">no output</div>

  <h1>StateMachine</h1>
  <div id="code0401" class="CODE" onclick="jc.codeClick(this);" contentEditable>
    v('count',-1);
    stateMachine('myStateMachine')<p>
    .state('state1').initial()<p>
    &nbsp;&nbsp;.run('v("count",count+1)')<p>
    &nbsp;&nbsp;.transition('state2','count == 10')<p>
    .state('state2')<p>
    &nbsp;&nbsp;.run('v("count",count-1)')<p>
    &nbsp;&nbsp;.transition('state1','count == 0')<p>
    .end().view();
  </div>
  <div id="out0401" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
  <div id="code0402" class="CODE" onclick="jc.codeClick(this);" contentEditable>
    output<p>
    .p(jc.simulation.runWhile("jc.simulation.time$s < 10"))<p>
    .p(myStateMachine.view({log:true}))<p>
    .p(count)
  </div>
  <div id="out0402" class="OUTPUT" onclick="jc.outClick(this)">no output</div>
