<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:svg="http://www.w3.org/2000/svg">
<head>
  <HTA:APPLICATION
    APPLICATIONNAME="Operating System Version par exemple"
    SCROLL="yes"
    SINGLEINSTANCE="yes">
  <link rel="stylesheet" href="jcalc.css" type="text/css" src="jcalc.css"/>
</head>

<script src="jquery.js"></script>
<script src="jcalc.js"></script>
<script src="axe.js"></script>
<script src="jcalcEdi.js"></script>

<body designMode="on">
  <div id="localToolBar" hidden="true"><span id=codeId>codeid</span><button>new</button><button onclick="showOutputHtml(this);">show output html</button></div>
  <nav id="mainToolBar">
    <input id="autoRun" type="checkbox" onclick="if (this.checked) {execAll()};">AutoRun</input>
  </nav>
  <h1>basics</h1>
  <div id="code0001" class="CODE" onclick="codeClick(this);" contentEditable>3+5</div>
  <div id="out0001" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <div id="test0001" class="TEST"><div class="INFO" >8</div></div>
  <h1>jCalc lib</h1>
  <div id="code0102" class="CODE" onclick="codeClick(this);" contentEditable>v("hauteurTotale$mm",f("hauteur$mm+5"))<p>v("hauteur$mm",3)<p>hauteurTotale$mm</div>
  <div id="out0102" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <div id="test0102" class="TEST"><div class="INFO" >8</div></div>
  <div id="code0103" class="CODE" onclick="codeClick(this);" contentEditable>v("hauteur$mm",3)</div>
  <div id="out0103" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <div id="code0104" class="CODE" onclick="codeClick(this);" contentEditable>v("hauteur$mm")</div>
  <div id="out0104" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <div id="code0105" class="CODE" onclick="codeClick(this);" contentEditable>
    table("myTable")<p>
    &nbsp;&nbsp;.add({_id:"apple" ,quantity:250,price:0.5,amount:f("quantity*price")})<p>
    &nbsp;&nbsp;.add({_id:"orange",quantity:100,price:0.7,amount:f("quantity*price")});
  </div>
  <div id="out0105" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <div id="code0106" class="CODE" onclick="codeClick(this);" contentEditable>
    html()<p>
    &nbsp;&nbsp;.p("first § where we show that ",hauteurTotale$mm)<p>
    &nbsp;&nbsp;.p("second § where we show myTable",myTable.span({cols:{_id:1,amount:1}}))<p>
    &nbsp;&nbsp;.ul()<p>
    &nbsp;&nbsp;&nbsp;&nbsp;.li('list 1 the full table',myTable)<p>
    &nbsp;&nbsp;&nbsp;&nbsp;.li('list 2 a row',myTable.orange)<p>
    &nbsp;&nbsp;&nbsp;&nbsp;.li('list 3 a row as a list',myTable.orange.list())<p>
    &nbsp;&nbsp;.end()<p>
    &nbsp;&nbsp;.h('Titre 1').indent()<p>
    &nbsp;&nbsp;&nbsp;&nbsp;.h('Sous-Titre 1')<p>
    &nbsp;&nbsp;&nbsp;&nbsp;.h('Sous-Titre 2')<p>
    &nbsp;&nbsp;.indent(-1)<p>
    &nbsp;&nbsp;.h('Titre 2')
  </div>
  <div id="out0106" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <h1>JQuery</h1>
  <div id="code0201" class="CODE" onclick="codeClick(this);" contentEditable>$('#code0104')[0]</div>
  <div id="out0201" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <div id="test0201" class="TEST"><div class="INFO" >[jQuery of 1 elements]</div></div>
  <h1>Axe</h1>
  <div id="code0301" class="CODE" onclick="codeClick(this);" contentEditable>
    axe('lift')<p>
    .accelerate({t:5,a:1})<p>
    .accelerate({t:10,a:-1})<p>
    .accelerate({t:15,a:0})<p>
    .move({t:20,a:1,d:40,v1:5})
  </div>
  <div id="out0301" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  <div id="code0302" class="CODE" onclick="codeClick(this);" contentEditable>
    lift<p>
    .accelerate({t:33,a:1})<p>
  </div>
  <div id="out0302" class="OUTPUT" onclick="outClick(this)"><div class="OLD">no output</div></div>
  

  <h1>debug</h1>
  <div id="debug" class="DEBUG">
    <div>debug</div>
  </div>
  <div id="test">
    <div id="test 1">test 1
      <p id="p1_1">§ 1.1</p>
      <p id="p1_2">§ 1.2</p>
    </div>
    <div id="test 2">test 2
      <p id="p2_1">§ 2.1</p>
      <p id="p2_2">§ 2.2</p>
    </div>
  </div>


</body>
</html>